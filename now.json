{
  "version": 2,
  "name": "react-redux-app",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    { "src": "^/public/(.*)", "dest": "/public/$1" },
    { "src": "^/favicon.ico", "dest": "/favicon.ico" },
    { "src": "^/(.*)", "dest": "/index.html" }
  ]
}
